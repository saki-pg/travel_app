<div>
  <h3>登録済みルーム一覧</h3>

  <table>
    <thead>
      <tr>
        <th>ルーム画像</th>
        <th>ルーム名</th>
        <th>ルーム紹介</th>
        <th>料金</th>
        <th>作成日</th>
      </tr>
    </thead>

    <tbody>
      <% @post.each do |post| %>
        <tr>
          <td>
            <%= image_tag post.picture,:size => "200x200"%>
          </td>
          <td class="posts-name"><%=link_to post.name,post_path(post.id) %></td>
          <td><%= post.information %></td>
          <td>¥<%= post.price %></td>
          <td><%= post.created_at %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>